[
    {
        "kernelSource": {
            "name": "MIX2S",
            "repo": "https://github.com/PixelExperience-Devices/kernel_xiaomi_polaris",
            "branch": "thirteen",
            "device": "polaris",
            "defconfig": "polaris_defconfig"
        },
        "toolchains": [
            {
                "repo": "https://github.com/PixelExperience/prebuilts_clang_kernel_linux-x86_clang-r416183b",
                "branch": "fourteen",
                "name": "clang",
                "binaryEnv": [
                    "bin"
                ]
            },
            {
                "repo": "https://github.com/PixelExperience/prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
                "branch": "fourteen",
                "name": "aarch64",
                "binaryEnv": [
                    "bin"
                ]
            },
            {
                "repo": "https://github.com/PixelExperience/prebuilts_gcc_linux-x86_arm_arm-linux-androideabi-4.9",
                "branch": "fourteen",
                "name": "arm",
                "binaryEnv": [
                    "bin"
                ]
            }
        ],
        "enableCcache": true,
        "params": {
            "ARCH": "arm64",
            "CC": "clang",
            "externalCommands": {
                "CLANG_TRIPLE": "aarch64-linux-gnu-",
                "CROSS_COMPILE": "aarch64-linux-android-",
                "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
                "LD": "ld.lld",
                "AR": "llvm-ar",
                "NM": "llvm-nm",
                "OBJCOPY": "llvm-objcopy",
                "OBJDUMP": "llvm-objdump",
                "READELF": "llvm-readelf",
                "OBJSIZE": "llvm-size",
                "STRIP": "llvm-strip",
                "LDGOLD": "aarch64-linux-gnu-ld.gold",
                "LLVM_AR": "llvm-ar",
                "LLVM_DIS": "llvm-dis",
                "CONFIG_THINLTO": ""
            }
        },
        "AnyKernel3": {
            "use": true,
            "release": true
        },
        "enableKernelSU": true,
        "enableLXC": true
    }
]
